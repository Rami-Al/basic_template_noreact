    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "compass-mixins": "^0.12.10",
    "jquery": "^3.4.1",
    "popper.js": "^1.15.0"
  }
}